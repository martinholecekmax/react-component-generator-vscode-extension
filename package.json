{
  "name": "react-component-generator",
  "displayName": "react-component-generator",
  "description": "Create files for React component in selected directory.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-component-generator.createReactComponent",
        "title": "React Component Generator"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "react-component-generator.createReactComponent",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "command": "react-component-generator.createReactComponent",
          "when": "explorerResourceIsFolder  || resourceFilename != ''",
          "group": "create-react-component"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}

